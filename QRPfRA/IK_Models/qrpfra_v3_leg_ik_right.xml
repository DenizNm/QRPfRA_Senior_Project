<mujoco model="qrpfra_v3_ik_model">
    <compiler angle="degree" coordinate="local" inertiafromgeom="true" convexhull="true"/>
    <option integrator="RK4" timestep="0.01" gravity="0 0 -9.81" density="1.24"/>

    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="0" condim="3" density="1.24" friction="1 0.5 0.5" margin="0.01" rgba="0.8 0.6 0.4 1"/>

        <default class="servo_motors">
            <position ctrllimited="true" ctrlrange="-100 100" gear="95"/>
        </default>

        <default class="joint_def">
            <joint pos="0 0 0" range="-60 60" type="hinge" damping="150"/>
        </default>

        <default class="Left_foot_Site">
            <site pos="-0.015 0.12 0" type="sphere" size="0.02"/>
        </default>

        <default class="Right_foot_Site">
            <site pos="0.015 0.12 0" type="sphere" size="0.02"/>
        </default>

    </default>

